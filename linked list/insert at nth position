#include<iostream>
//#include<conio>
using namespace std;

struct node{
	int data;
	node* link;
};
struct node* head;

void calc(int p,int x)
{
    int c=0;
	node* temp= new node;
    node* ptr;
    node* cptr=head;
    ptr=head;
	temp->data=x;
    temp->link=NULL;
    if(ptr==NULL)
    {
       // temp->link=head;
        head=temp;
    
    }
    else if(p==1)
    {
        temp->link=head;
        head=temp;
    }  
    else
    { c=1;
        while(ptr->link!=NULL)
        {
            ptr=ptr->link;
            cptr=cptr->link;
            c++;
            if(c==p)
            {
               cptr=cptr->link;
               ptr->link=temp;
               //cptr=cptr->link;
               temp->link=cptr;

                break;
            }    
        } 
            
    }

}

void print()
{
	node* temp2=head;
	while(temp2!=NULL)
	{
		cout<<temp2->data;
		temp2=temp2->link;
		cout<<" ";
	}
	cout<<endl;

}
int main() {
    head = NULL;
    int n;
    cout<<"enter the number of variables  ";
    cin>>n;
    int x,p;
    for(int i=0;i<n;i++)
    {
        cout<<"enter the position "<<endl;
        cin>>p;
    	cout<<endl<<"enter the data ";
    	cin>>x;
    	calc(p,x);
    	print();
    }	
    

    return 0;
}
